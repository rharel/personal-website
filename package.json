{
  "name": "www.rharel.com",
  "version": "1.0.0",
  "description": "Source of www.rharel.com",
  "scripts": {
    "clean": "rm -rf templates_compiled && rm -rf distribution",
    "watch:eleventy": "eleventy --input=templates --output=templates_compiled --watch",
    "serve:parcel": "parcel serve --no-cache --dist-dir distribution",
    "build:eleventy": "eleventy --input=templates --output=templates_compiled",
    "build:parcel": "parcel build --no-cache",
    "build": "run-s clean build:eleventy build:parcel"
  },
  "keywords": [],
  "author": "Raoul Harel <admin@rharel.com>",
  "license": "All rights reserved",
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@parcel/transformer-sass": "2.4.1",
    "parcel": "2.4.1",
    "parcel-resolver-ignore": "2.0.0",
    "sharp": "^0.29.3"
  },
  "targets": {
    "default": {
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      },
      "sourceMap": false,
      "source": "./templates_compiled/index.html",
      "distDir": "./distribution"
    }
  },
  "parcelIgnore": [
    "/apple-touch-icon.png",
    "/favicon-16x16.png",
    "/favicon-32x32.png",
    "/safari-pinned-tab.svg",
    "/site.webmanifest"
  ]
}
